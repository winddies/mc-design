{
  "name": "@bud-fe/mc-app",
  "version": "1.0.0",
  "private": true,
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "less"
  },
  "scripts": {
    "build": "npm run build:weapp",
    "build:weapp": "taro build --type weapp",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "lint": "eslint src --ext .js,.ts,.jsx,.tsx && stylelint 'src/**/*.{css,less,scss}'",
    "lint:fix": "eslint src --fix --ext .js,.ts,.jsx,.tsx && stylelint --fix 'src/**/*.{css,less,scss}'",
    "prettier": "npx prettier --write src"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "Bud FE",
  "dependencies": {
    "@babel/runtime": "^7.20.0",
    "@bud-fe/mc-ui": "workspace:^",
    "@taro-hooks/plugin-react": "^2.0.2",
    "@taro-hooks/shared": "^2.0.6",
    "@taro-hooks/ahooks": "^2.1.0",
    "@tarojs/components": "3.6.16",
    "@tarojs/helper": "3.6.16",
    "@tarojs/plugin-framework-react": "3.6.16",
    "@tarojs/plugin-html": "3.6.16",
    "@tarojs/plugin-platform-h5": "3.6.16",
    "@tarojs/plugin-platform-weapp": "3.6.16",
    "@tarojs/react": "3.6.16",
    "@tarojs/runtime": "3.6.16",
    "@tarojs/shared": "3.6.16",
    "@tarojs/taro": "3.6.16",
    "classnames": "^2.3.2",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "taro-hooks": "^2.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@bud-fe/f2elint": "^0.3.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.8",
    "@tarojs/cli": "3.6.16",
    "@tarojs/taro-loader": "^3.6.16",
    "@tarojs/webpack5-runner": "3.6.16",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/webpack-env": "^1.18.0",
    "babel-plugin-import": "^1.13.5",
    "babel-preset-taro": "3.6.16",
    "eslint-config-taro": "^3.6.16",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-taro": "^3.3.20",
    "react-refresh": "^0.14.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2"
  }
}